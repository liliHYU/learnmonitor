model Project {
    id     String @unique
    name   String @unique
    leader String
    desc   String
    ctime  DateTime
}

model Session {
    id         String   @unique
    user_id    String
    ip         String
    province   String
    path       String
    page_title String
    platform   String
    stay_time  Int
    terminal   String
    language   String
    events     String   
    etime      DateTime
    ltime      DateTime
    ctime      DateTime @default(now()) @updatedAt
}

model Log {
    id            String   @unique
    ctime         DateTime @default(now()) @updatedAt
    otime         DateTime
    type          String
    sub_type      String
    session_id    String
    ascription_id String
    breadcrumb    String  
    data          String   
    path          String
    page_title    String
    platform      String
    user_agent    String  
    language      String
}

model BreadCrumb {
    id       String @unique
    event_id String
    type     String
    message  String
    level    String
    time     DateTime
}
